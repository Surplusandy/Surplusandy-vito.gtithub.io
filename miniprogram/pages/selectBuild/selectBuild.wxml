<!--pages/selectBuild/selectBuild.wxml-->
<scroll-view scroll-x  class="scroll_view">
   <view class="tab">
       <view class="tab_item {{ tabNow === index ? 'select' : ''}} "
       data-id="{{ index }}"
       wx:for="{{tabList}}" wx:key="index" 
       bindtap="selectTab"
       data-id="{{index}}">
       {{item}}
       </view>
   </view>
</scroll-view>

<view wx:if="{{tabNow===0}}" class="body">
   <view class="item" wx:for="{{16}}" bindtap="selectBuild" data-index="{{ index }}" wx:key="index">
     {{index+1}}栋
   </view>
</view>

